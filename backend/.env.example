# Server Configuration
PORT=3001

# Ethereum Configuration
# Backend signer wallet (for gasless claim relay)
PRIVATE_KEY=your_backend_wallet_private_key

# Owner wallet (for Manifold setTokenURI - this is the wallet that deployed the NFT contract)
# This is needed to update NFT metadata with observations
OWNER_PRIVATE_KEY=your_owner_wallet_private_key

RPC_URL=https://eth-mainnet.g.alchemy.com/v2/your-alchemy-key

# Contract Addresses
NFT_CONTRACT=0x83e2654994264333e6fdfe2e43eb862866746041
CLAIMER_CONTRACT=0x_your_deployed_claimer_contract

# Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key

# CORS - Your frontend domain
FRONTEND_URL=http://localhost:8080

# Arweave Configuration (for permanent metadata storage)
# Option 1: Path to Arweave keyfile JSON
ARWEAVE_KEY_FILE=./arweave-keyfile.json
# Option 2: Base64 encoded Arweave key (for environments that don't support files)
# ARWEAVE_KEY_BASE64=your_base64_encoded_key

# Optional: Use Bundlr/Irys for cheaper Arweave uploads
# BUNDLR_NODE=https://node1.bundlr.network
# BUNDLR_CURRENCY=ethereum
